#include "sofle_tb.dtsi"

&default_transform {
    col-offset = <6>;
};

&kscan0 {
    col-gpios
        = <&pro_micro 6 GPIO_ACTIVE_HIGH>
        , <&pro_micro 4 GPIO_ACTIVE_HIGH>
        , <&pro_micro 11 GPIO_ACTIVE_HIGH>
        , <&pro_micro 2 GPIO_ACTIVE_HIGH>
        , <&pro_micro 29 GPIO_ACTIVE_HIGH>
        , <&pro_micro 31 GPIO_ACTIVE_HIGH>
        ;
};

&right_encoder {
    status = "okay";
};